[Unit]
Description=<%= @description %>

[Service]
Type=oneshot
<%- @environment.each do |var| -%>
Environment="<%= var %>"
<%- end -%>
User=<%= @user %>
<%- if @command.is_a? String -%>
ExecStart=<%= @command %>
<%- else -%>
<%- @command.each do |cmd| -%>
ExecStart=<%= cmd %>
<%- end -%>
<%- end -%>
