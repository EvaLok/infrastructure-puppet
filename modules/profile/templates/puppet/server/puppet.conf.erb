[server]
vardir = <%= @server_var_dir %>
logdir = <%= @server_log_dir %>
rundir = <%= @server_run_dir %>
pidfile = <%= @server_run_dir %>/puppetserver.pid
codedir = <%= @code_path %>
environment = <%= @environment %>

[master]
storeconfigs = true
storeconfigs_backend = puppetdb
reports = store,puppetdb
