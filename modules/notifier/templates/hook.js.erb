module.exports = function( server, exec ) {
<%- @listen_for.each do |entry| -%>
<%- if entry.key? 'branch' -%>
  server.on( "<%= @github_repository %>/push/heads/<%= entry['branch'] %>", exec );
<%- end -%>
<%- if entry.key? 'tag' -%>
  server.on( "<%= @github_repository %>/push/tags/*", function ( data ) {
    if ( data.tag.match( /<%= entry['branch'] %>/ ) ) {
      exec( data );
    }
  } );
<%- end -%>
<%- end -%>
};
